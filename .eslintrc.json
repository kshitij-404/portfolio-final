{
  "extends": "airbnb",
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "env": {
    "browser": true,
    "node": true,
    "jquery": true
  },
  "rules": {
    "comma-dangle": 0,
    "max-len": 0,
    "no-new": 0,
    "no-prototype-builtins": 0,
    "no-param-reassign": 0,
    "func-names": 0,
    "quote-props": 0,
    "no-lonely-if": 0,
    "no-underscore-dangle": 0
  }
}